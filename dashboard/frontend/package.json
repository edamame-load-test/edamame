    "babel-jest": "^29.6.1",
    "jest-transform-stub": "^2.0.0",
    "react-test-renderer": "^18.2.0",
    "shx": "^0.3.4",
    "tailwindcss": "^3.2.7"
  },
  "jest": {
    "transform": {
